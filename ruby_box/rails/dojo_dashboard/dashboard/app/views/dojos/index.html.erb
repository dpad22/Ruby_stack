<h1>Listing <%= @dojos.length%> Dojos</h1>
<table class="table table-striped">
    <thead>
            <tr>
            <th scope="col">Coding Dojo Branch</th>
            <th scope="col">Street</th>
            <th scope="col">City</th>
            <th scope="col">State</th>
            <th scope="col">Actions</th>
            </tr>
    </thead>
    <tbody>
    <% @dojos.each do |dojo| %>
        <tr>
        <td> <%= dojo.branch %> </td>
        <td> <%= dojo.street %> </td>
        <td> <%= dojo.city %></p> </td>
        <td> <%= dojo.state %></p> </td>
        <td> 
            <a href="/dojos/<%= dojo.id %>">Show</a> |
            <a href="/dojos/<%= dojo.id %>/edit">Edit</a> |
            <a href="/dojos/<%= dojo.id %>/delete" data-method="delete">Destroy</a> 
        </td>
        </tr>
    <% end %>
    </tbody>
</table>
<a href="/dojos/new">New Dojo</a>
