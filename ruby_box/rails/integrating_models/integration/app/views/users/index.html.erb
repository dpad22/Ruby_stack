<h1>Users#index</h1>

<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Created At</th>
        <th>Updated At</th>
    </tr>
    <% @users.each do |user| %>
    <tr>
        <th> <%= user.id %> </th>
        <th> <%= user.name %> </th>
        <th> <%= user.created_at %> </th>
        <th> <%= user.updated_at %> </th>
    </tr>
    <% end %>
</table>
